<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.javadaily.product.mapper.ProductMapper">

    <delete id="deleteByCode">
        delete from product WHERE PRODUCT_CODE = #{productCode}
    </delete>

    <select id="selectByCode"  resultType="com.javadaily.product.po.Product">
        select *  from product WHERE PRODUCT_CODE = #{productCode}
    </select>

</mapper>